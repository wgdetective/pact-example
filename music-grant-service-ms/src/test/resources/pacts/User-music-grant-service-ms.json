{
  "consumer": {
    "name": "User"
  },
  "interactions": [
    {
      "description": "add song",
      "providerStates": [
        {
          "name": "no songs exist"
        }
      ],
      "request": {
        "body": {
          "author": "Rick Astley",
          "name": "Never Gonna Give You Up"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "PUT",
        "path": "/v1/song"
      },
      "response": {
        "body": {
          "author": "Rick Astley",
          "id": 1,
          "name": "Never Gonna Give You Up"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "matchingRules": {
          "body": {
            "$.author": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            },
            "$.id": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "number"
                }
              ]
            },
            "$.name": {
              "combine": "AND",
              "matchers": [
                {
                  "match": "type"
                }
              ]
            }
          }
        },
        "status": 200
      }
    },
    {
      "description": "add song",
      "providerStates": [
        {
          "name": "song already exist"
        }
      ],
      "request": {
        "body": {
          "author": "Rick Astley",
          "name": "Never Gonna Give You Up"
        },
        "headers": {
          "Content-Type": "application/json"
        },
        "method": "PUT",
        "path": "/v1/song"
      },
      "response": {
        "status": 409
      }
    }
  ],
  "metadata": {
    "pact-jvm": {
      "version": "4.4.5"
    },
    "pactSpecification": {
      "version": "3.0.0"
    }
  },
  "provider": {
    "name": "music-grant-service-ms"
  }
}
